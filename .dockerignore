frontend/build/
frontend/dist/

# Docker files (not needed in build context)
Dockerfile.dev
docker-compose.override.yml
.dockerignore

# Development and testing files
.coverage
.pytest_cache/
.nyc_output
.cache
.parcel-cache
.eslintcache

# Secrets and keys
secrets/
*.key
*.pem
*.crt
*.p12
*.pfx

# Backup and temporary files
*.backup
*.old
*.tmp
*.temp
*.bak

# Documentation (keep important docs but exclude generated)
docs/_build/
docs/build/

# Local development files
.local
