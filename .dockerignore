frontend/node_modules/
frontend/.next/
frontend/out/

# Python cache and compiled files
__pycache__/
*.pyc
*.pyo
*.pyd
.Python
*.so
backend/__pycache__/
backend/*.pyc

# IDE files
.vscode/
.idea/
*.swp
*.swo
*~

# OS generated files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

